{
  "name": "@open-dashboard/shared",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./types/core.ts",
  "types": "./types/core.ts",
  "exports": {
    ".": "./types/core.ts",
    "./types": "./types/core.ts",
    "./types/*": "./types/*",
    "./utils": "./utils/index.ts",
    "./utils/*": "./utils/*"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "~5.8.3"
  }
}

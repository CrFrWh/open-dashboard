{
  "name": "open-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*",
    "shared"
  ],
  "scripts": {
    "dev": "cd apps/demo && bun run dev",
    "dev:demo": "cd apps/demo && bun run dev",
    "build": "tsc -b",
    "build:packages": "tsc -b packages/*/tsconfig.json shared/tsconfig.json",
    "build:clean": "tsc -b --clean",
    "typecheck": "tsc -b",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.1.13",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@vitejs/plugin-react": "^5.0.3",
    "babel-plugin-react-compiler": "^19.0.0-beta-94eed63c-20241121",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.16.0",
    "vite": "npm:rolldown-vite@7.1.12"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.12"
  },
  "resolutions": {
    "vite": "npm:rolldown-vite@7.1.12"
  }
}
